{
  "name": "jspicl-mario-sample",
  "version": "0.0.1",
  "description": "A simple Mario sample game using jspicl to transpile Javascript",
  "scripts": {
    "restore-cart": "cp mario-starter-cart.p8 \"$npm_package_config_dest\"",
    "prebuild": "npm -s run restore-cart -n",
    "build": "rollup -c",
    "lint": "eslint src",
    "start": "npm run build -- -w"
  },
  "author": "Agron Kabashi",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^3.19.0",
    "esprima": "^3.1.3",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-conditional": "^1.1.1",
    "rollup-plugin-includepaths": "^0.2.2",
    "rollup-plugin-jspicl": "^1.2.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.2",
    "rollup-watch": "^3.2.2"
  },
  "config": {
    "entry": "src/bootstrap.js",
    "dest": "/Users/agronkabashi/Library/Application Support/pico-8/carts/mario-sample.p8"
  }
}

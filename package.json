{
  "name": "jspicl-mario-sample",
  "version": "0.0.1",
  "description": "A simple Mario sample game using jspicl to transpile Javascript",
  "scripts": {
    "restore-cart": "cross-env copyfiles mario-sample.p8 \"$npm_package_config_destFolder\"",
    "prebuild": "npm run restore-cart -- -s",
    "build": "rollup -c",
    "lint": "eslint src",
    "start": "npm run build -- -w"
  },
  "author": "Agron Kabashi",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "copyfiles": "^1.2.0",
    "cross-env": "^5.1.1",
    "eslint": "^4.11.0",
    "esprima": "^4.0.0",
    "rollup": "^0.51.5",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-buble": "^0.17.0",
    "rollup-plugin-includepaths": "^0.2.2",
    "rollup-plugin-jspicl": "^4.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^4.3.1"
  },
  "config": {
    "input": "src/bootstrap.js",
    "destFolder": "/Users/agronkabashi/Library/Application Support/pico-8/carts",
    "destName": "mario-sample.p8"
  }
}
